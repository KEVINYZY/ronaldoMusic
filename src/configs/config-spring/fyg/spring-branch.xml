<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	 http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <!-- this spring-branch is managed by fyg -->

    <bean id="initDao" class="com.cheery.dao.dataInit.DataInitImpl">
        <property name="sessionFactory" ref="sessionFactory"/>
    </bean>

    <bean id="initSer" class="com.cheery.service.DataInitService">
        <property name="initDao" ref="initDao"/>
    </bean>

    <bean id="userDao" class="com.cheery.dao.user.UserDaoImpl">
        <property name="sessionFactory" ref="sessionFactory"/>
    </bean>

    <bean id="userSer" class="com.cheery.service.UserService">
        <property name="userDao" ref="userDao"/>
    </bean>

    <bean id="playlistDao" class="com.cheery.dao.playlist.PlaylistDaoImpl">
        <property name="sessionFactory" ref="sessionFactory"/>
    </bean>

    <bean id="playSer" class="com.cheery.service.PlayListService">
        <property name="playlistDao" ref="playlistDao"/>
    </bean>

</beans>

