<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cheery.model">
    <!--name：实体类名,table：对应数据库表名-->
    <class name="MvEntity" table="mv">
        <!-- 配置主键映射 -->
        <!--name：实体类的属性名,type：数据类型,column：数据库表的字段名-->
        <id name="mvId" column="mvId" type="integer">
            <!-- 设置主键生成方式 -->
            <generator class="native"></generator>
        </id>
        <!-- 配置其他属性 -->
        <!--name：实体类的属性名,type：数据类型,column：数据库表的字段名-->
        <property name="mvName" column="mvName" type="java.lang.String"/>
        <property name="mvPic" column="mvPic" type="java.lang.String"/>
        <property name="mvUrl" column="mvUrl" type="java.lang.String"/>
        <property name="mvAuthor" column="mvAuthor" type="java.lang.String"/>
        <property name="mvType" column="mvType" type="java.lang.String"/>
        <property name="time" column="time" type="java.lang.String"/>
        <property name="explain" column="explain" type="java.lang.String"/>
    </class>
</hibernate-mapping>