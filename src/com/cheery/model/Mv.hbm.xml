<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cheery.model">
    <!--name：实体类名,table：对应数据库表名-->
    <class name="MvEntity" table="mv">
        <!-- 配置主键映射 -->
        <!--name：实体类的属性名,type：数据类型,column：数据库表的字段名-->
        <id name="mvId" type="integer">
            <column name="mvId"></column>
            <!-- 设置主键生成方式 -->
            <generator class="native"></generator>
        </id>
        <!-- 配置其他属性 -->
        <!--name：实体类的属性名,type：数据类型,column：数据库表的字段名-->
        <property name="mvName" type="java.lang.String">
            <column name="mvName"></column>
        </property>
        <property name="mvPic" type="java.lang.String">
            <column name="mvPic"></column>
        </property>
        <property name="mvUrl" type="java.lang.String">
            <column name="mvUrl"></column>
        </property>
        <property name="mvAuthor" type="java.lang.String">
            <column name="mvAuthor"></column>
        </property>
        <property name="mvType" type="java.lang.String">
            <column name="mvType"></column>
        </property>
        <property name="time" type="java.lang.String">
            <column name="time"></column>
        </property>
        <property name="explain" type="java.lang.String">
            <column name="explain"></column>
        </property>
    </class>
</hibernate-mapping>