<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cheery.model">
    <!--name：实体类名,table：对应数据库表名-->
    <class name="PlaylistEntity" table="playlist">
        <!-- 配置主键映射 -->
        <!--name：实体类的属性名,type：数据类型,column：数据库表的字段名-->
        <id name="plId" type="integer">
            <column name="plId"></column>
            <!-- 设置主键生成方式 -->
            <generator class="native"></generator>
        </id>
        <!-- 多对一 -->
        <many-to-one name="user" column="userId" class="UserEntity" unique="true" not-found="ignore"></many-to-one>
        <!-- 多对多 -->
        <many-to-one name="song" column="soId" class="SongEntity"></many-to-one>
        <many-to-one name="detail" column="deId" class="DetailEntity"></many-to-one>
    </class>
</hibernate-mapping>